<script>
    $(document).ready(function () {

        var usernameforresend = "";

        $("#country-dropdown").change(function () {
            if ($(this).val() == "Pakistan") {
                $("#city-textbox").hide();
                $("#city-textbox").removeAttr("name");
                $("#city-dropdown").show();
                $("#city-dropdown").attr("name", "city");
            }
            else {
                $("#city-textbox").show();
                $("#city-textbox").attr("name","city");
                $("#city-dropdown").hide();
                $("#city-dropdown").removeAttr("name");
            }
        });

        $("#country-dropdown-hall").change(function () {
            if ($(this).val() == "Pakistan") {
                $("#city-textbox-hall").hide();
                $("#city-textbox-hall").removeAttr("name");
                $("#city-dropdown-hall").show();
                $("#city-dropdown-hall").attr("name", "city");
            }
            else {
                $("#city-textbox-hall").show();
                $("#city-textbox-hall").attr("name", "city");
                $("#city-dropdown-hall").hide();
                $("#city-dropdown-hall").removeAttr("name");
            }
        });

        $("#country-dropdown-caterer").change(function () {
            if ($(this).val() == "Pakistan") {
                $("#city-textbox-caterer").hide();
                $("#city-textbox-caterer").removeAttr("name");
                $("#city-dropdown-caterer").show();
                $("#city-dropdown-caterer").attr("name", "city");
            }
            else {
                $("#city-textbox-caterer").show();
                $("#city-textbox-caterer").attr("name", "city");
                $("#city-dropdown-caterer").hide();
                $("#city-dropdown-caterer").removeAttr("name");
            }
        });

        $("#regis-form").submit(function (e) {
            e.preventDefault();
            if ($("#regis-form").valid()) {
                //alert(JSON.stringify($("#regis-form").serializeObject()));
                $.ajax({
                    url: '/Account/Registration',
                    data: { 'json': JSON.stringify($("#regis-form").serializeObject()) },
                    type: "post",
                    cache: false,
                    beforeSend: function () {
                        $("#pluswrap").fadeIn(500)
                        $("#btn-submit").attr("disabled", "disabled");
                    },
                    success: function (savingStatus) {
                        usernameforresend = $("#username").val();
                        $("#pluswrap").fadeOut(500, function () {

                            if (ShowNotification(savingStatus, $("#main-container"),2)) {
                                $("#regis-section").hide();
                                $("#emailconfirmation-area").fadeIn(500);
                                
                            }
                            $("#btn-submit").removeAttr("disabled");
                            
                        });

                    },
                    error: function (xhr, ajaxOptions, thrownError) {
                        alert(xhr.responseText);
                        $("#pluswrap").fadeOut(500);
                        $("#btn-submit").removeAttr("disabled");

                    }
                });
            }
        });


        $("#hall-regis-form").submit(function (e) {
            e.preventDefault();
            if ($("#hall-regis-form").valid()) {
                //alert(JSON.stringify($("#hall-regis-form").serializeObject()));
                var amount_val = $("#amount").val();
                $("#amount").val($("#amount").val() + " " + $("#hall-currency").val());
                $.ajax({
                    url: '/Account/HallRegistration',
                    data: { 'json': JSON.stringify($("#hall-regis-form").serializeObject()) },
                    type: "post",
                    cache: false,
                    beforeSend: function () {
                        $("#pluswrap").fadeIn(500)
                        $("#btn-submit-hall").attr("disabled", "disabled");
                    },
                    success: function (savingStatus) {
                        if (Iserror(savingStatus) == "error") {
                            $("#pluswrap").fadeOut(500, function () {

                                usernameforresend = $("#usernamehall").val();
                                if (ShowNotification(savingStatus, $("#main-container"), 2)) {
                                    $("#regis-section").hide();
                                    $("#emailconfirmation-area").fadeIn(500);

                                }

                                $("#btn-submit-hall").removeAttr("disabled");

                            });
                        }
                        else {
                            var myID = Iserror(savingStatus);

                            var files = $("#hallimages")[0].files;
                            //var myID = 3; //uncomment this to make sure the ajax URL works
                            if (files.length > 0) {
                                //alert("in");
                                if (window.FormData !== undefined) {
                                    var data = new FormData();
                                    for (var x = 0; x < files.length; x++) {
                                        data.append("file" + x, files[x]);
                                    }

                                    $.ajax({
                                        type: "POST",
                                        url: '/Account/UploadFileHall?id=' + myID,
                                        contentType: false,
                                        processData: false,
                                        data: data,
                                        success: function (result) {
                                            $("#pluswrap").fadeOut(500, function () {

                                                usernameforresend = $("#usernamehall").val();
                                                if (ShowNotification(result, $("#main-container"), 2)) {
                                                    $("#regis-section").hide();
                                                    $("#emailconfirmation-area").fadeIn(500);

                                                }

                                                $("#btn-submit-hall").removeAttr("disabled");

                                            });
                                        },
                                        error: function (xhr, status, p3, p4) {
                                            alert(xhr.responseText);
                                            $("#pluswrap").fadeOut(500);
                                            $("#btn-submit-hall").removeAttr("disabled");
                                        }
                                    });
                                } else {
                                    alert("This browser doesn't support HTML5 file uploads!");
                                }
                            }

                            else {
                                $("#pluswrap").fadeOut(500, function () {
                                    ShowNotification(savingStatus, $("#main-container"), 2);
                                    usernameforresend = $("#usernamehall").val();
                                    $("#regis-section").hide();
                                    $("#emailconfirmation-area").fadeIn(500);
                                });
                            }
                        }

                    },
                    error: function (xhr, ajaxOptions, thrownError) {
                        alert(xhr.responseText);
                        $("#pluswrap").fadeOut(500);
                        $("#btn-submit-hall").removeAttr("disabled");

                    }
                });

                $("#amount").val(amount_val);
            }
        });

        $("#caterer-regis-form").submit(function (e) {
            e.preventDefault();
            if ($("#caterer-regis-form").valid()) {
                //alert(JSON.stringify($("#caterer-regis-form").serializeObject()));
                var rate_val = $("#rate").val();
                $("#rate").val($("#rate").val() + " " + $("#caterer-currency").val());
                $.ajax({
                    url: '/Account/CatererRegistration',
                    data: { 'json': JSON.stringify($("#caterer-regis-form").serializeObject()) },
                    type: "post",
                    cache: false,
                    beforeSend: function () {
                        $("#pluswrap").fadeIn(500)
                        $("#btn-submit-caterer").attr("disabled", "disabled");
                    },
                    success: function (savingStatus) {
                        if (Iserror(savingStatus) == "error") {
                            $("#pluswrap").fadeOut(500, function () {

                                usernameforresend = $("#usernamecaterer").val();
                                if (ShowNotification(savingStatus, $("#main-container"), 2)) {
                                    $("#regis-section").hide();
                                    $("#emailconfirmation-area").fadeIn(500);

                                }

                                $("#btn-submit-caterer").removeAttr("disabled");

                            });
                        }
                        else {
                            var myID = Iserror(savingStatus);

                            var files = $("#catererimages")[0].files;
                            //var myID = 3; //uncomment this to make sure the ajax URL works
                            if (files.length > 0) {
                                //alert("in");
                                if (window.FormData !== undefined) {
                                    var data = new FormData();
                                    for (var x = 0; x < files.length; x++) {
                                        data.append("file" + x, files[x]);
                                    }

                                    $.ajax({
                                        type: "POST",
                                        url: '/Account/UploadFileCaterer?id=' + myID,
                                        contentType: false,
                                        processData: false,
                                        data: data,
                                        success: function (result) {
                                            $("#pluswrap").fadeOut(500, function () {

                                                usernameforresend = $("#usernamecaterer").val();
                                                if (ShowNotification(result, $("#main-container"), 2)) {
                                                    $("#regis-section").hide();
                                                    $("#emailconfirmation-area").fadeIn(500);

                                                }

                                                $("#btn-submit-caterer").removeAttr("disabled");

                                            });
                                        },
                                        error: function (xhr, status, p3, p4) {
                                            alert(xhr.responseText);
                                            $("#pluswrap").fadeOut(500);
                                            $("#btn-submit-caterer").removeAttr("disabled");
                                        }
                                    });
                                } else {
                                    alert("This browser doesn't support HTML5 file uploads!");
                                }
                            }

                            else {
                                $("#pluswrap").fadeOut(500, function () {
                                    ShowNotification(savingStatus, $("#main-container"), 2);
                                    usernameforresend = $("#usernamecaterer").val();
                                    $("#regis-section").hide();
                                    $("#emailconfirmation-area").fadeIn(500);
                                });
                            }
                        }

                    },
                    error: function (xhr, ajaxOptions, thrownError) {
                        alert(xhr.responseText);
                        $("#pluswrap").fadeOut(500);
                        $("#btn-submit-caterer").removeAttr("disabled");

                    }
                });

                $("#rate").val(rate_val);
            }
        });

        $("#regis-form").validate({
            rules: {
                fname: {
                    minlength: 3,
                    required: true
                },
                lname: {
                    minlength: 3,
                    required: true
                },
                username: {
                    minlength: 5,
                    required: true,
                },
                email: {
                    required: true,
                },
                password: {
                    minlength: 8,
                    required: true
                },
                repassword: {
                    equalTo: "#password"
                }
                ,
                dob: {
                    required: true
                },
                city: {
                    required: true
                }
            },
            messages: {
                repassword: {
                    equalTo : "Password does not matched !"
                }
            },
            showErrors: function (errorMap, errorList) {
                $.each(this.successList, function (index, value) {
                    return $(value).popover("hide");
                });
                return $.each(errorList, function (index, value) {
                    var _popover;
                    console.log(value.message);
                    _popover = $(value.element).popover({
                        trigger: "manual",
                        placement: "top",
                        content: value.message,
                        template: "<div class=\"popover\"><div class=\"arrow\"></div><div class=\"popover-inner\"><div class=\"popover-content\"><p></p></div></div></div>"
                    });
                    _popover.data("bs.popover").options.content = value.message;
                    return $(value.element).popover("show");
                });
            }
        });

        $("#resend-click").click(function () {

            $.ajax({
                url: '/Account/ResendEmailVerification',
                data: { 'username': usernameforresend },
                type: "post",
                cache: false,
                beforeSend: function () {
                    $("#loadericon").fadeIn(300);
                },
                success: function (savingStatus) {
                    $("#loadericon").fadeOut();
                    if (savingStatus == "Sent") {
                        $("#tickicon").fadeIn(300);
                        setTimeout(function () { $("#tickicon").fadeOut(300); }, 2000);
                    }

                    else {
                        $("#crossicon").fadeIn(300);
                        setTimeout(function () { $("#crossicon").fadeOut(300); }, 2000);
                    }


                },
                error: function (xhr, ajaxOptions, thrownError) {
                    $("#loadericon").fadeOut();
                    $("#crossicon").fadeIn(300);
                    alert(xhr.responseText);
                    setTimeout(function () { $("#crossicon").fadeOut(300); }, 2000);

                }
            });

        });

        $("input[name='dob']").keypress(function (e) {
            e.preventDefault();
        });

        $(".timepicker").keypress(function (e) {
            e.preventDefault();
        })
        $(".timepicker").timepicker({
          
        });


        $("#hallimages").on('change', function (e) {
            //Get count of selected files
            var countFiles = $(this)[0].files.length;
            if (countFiles > 0) {
                if (countFiles <= 5) {
                    var imgPath = $(this)[0].value;
                    var extn = imgPath.substring(imgPath.lastIndexOf('.') + 1).toLowerCase();
                    var image_holder = $("#image-holder-hall");
                    image_holder.empty();
                    if (extn == "gif" || extn == "png" || extn == "jpg" || extn == "jpeg") {
                        if (typeof (FileReader) != "undefined") {
                            //loop for each file selected for uploaded.
                            for (var i = 0; i < countFiles; i++) {
                                var reader = new FileReader();
                                reader.onload = function (e) {
                                    $("<img />", {
                                        "src": e.target.result,
                                        "class": "thumb-image hall-thumb"
                                    }).appendTo(image_holder);
                                }
                                image_holder.show();
                                reader.readAsDataURL($(this)[0].files[i]);
                            }
                        } else {
                            alert("This browser does not support FileReader.");
                        }
                    } else {
                        alert("Please select only images");
                    }
                }
                else {
                    alert("Maximum 5 Images Allowed");
                    $("#hallimages").val("");
                    $(".hall-thumb").remove();
                }
            }
            else {
                $(".hall-thumb").remove();
            }

        });




        $("#catererimages").on('change', function (e) {
            //Get count of selected files
            var countFiles = $(this)[0].files.length;
            if (countFiles > 0) {
                if (countFiles <= 5) {
                    var imgPath = $(this)[0].value;
                    var extn = imgPath.substring(imgPath.lastIndexOf('.') + 1).toLowerCase();
                    var image_holder = $("#image-holder-caterer");
                    image_holder.empty();
                    if (extn == "gif" || extn == "png" || extn == "jpg" || extn == "jpeg") {
                        if (typeof (FileReader) != "undefined") {
                            //loop for each file selected for uploaded.
                            for (var i = 0; i < countFiles; i++) {
                                var reader = new FileReader();
                                reader.onload = function (e) {
                                    $("<img />", {
                                        "src": e.target.result,
                                        "class": "thumb-image caterer-thumb"
                                    }).appendTo(image_holder);
                                }
                                image_holder.show();
                                reader.readAsDataURL($(this)[0].files[i]);
                            }
                        } else {
                            alert("This browser does not support FileReader.");
                        }
                    } else {
                        alert("Please select only images");
                    }
                }
                else {
                    alert("Maximum 5 Images Allowed");
                    $("#catererimages").val("");
                    $(".caterer-thumb").remove();
                }
            }
            else {
                $(".caterer-thumb").remove();
            }

        });


        $("#hall-regis-form").validate({
            rules: {
                hallname: {
                    minlength: 3,
                    required: true
                },
                username: {
                    minlength: 5,
                    required: true,
                },
                email: {
                    required: true,
                },
                password: {
                    minlength: 8,
                    required: true
                },
                repassword: {
                    equalTo: "#password-hall"
                }
                ,
                city: {
                    required: true
                }
                ,
                capacity: {
                    required: true
                },
                amount: {
                    required: true
                }
            },
            messages: {
                repassword: {
                    equalTo: "Password does not matched !"
                }
            },
            showErrors: function (errorMap, errorList) {
                $.each(this.successList, function (index, value) {
                    return $(value).popover("hide");
                });
                return $.each(errorList, function (index, value) {
                    var _popover;
                    console.log(value.message);
                    _popover = $(value.element).popover({
                        trigger: "manual",
                        placement: "top",
                        content: value.message,
                        template: "<div class=\"popover\"><div class=\"arrow\"></div><div class=\"popover-inner\"><div class=\"popover-content\"><p></p></div></div></div>"
                    });
                    _popover.data("bs.popover").options.content = value.message;
                    return $(value.element).popover("show");
                });
            }
        });

        $("#caterer-regis-form").validate({
            rules: {
                caterername: {
                    minlength: 3,
                    required: true
                },
                username: {
                    minlength: 5,
                    required: true,
                },
                email: {
                    required: true,
                },
                password: {
                    minlength: 8,
                    required: true
                },
                repassword: {
                    equalTo: "#password-caterer"
                }
                ,
                city: {
                    required: true
                }
                ,
                capacity: {
                    required: true
                },
                rate: {
                    required: true
                }
            },
            messages: {
                repassword: {
                    equalTo: "Password does not matched !"
                }
            },
            showErrors: function (errorMap, errorList) {
                $.each(this.successList, function (index, value) {
                    return $(value).popover("hide");
                });
                return $.each(errorList, function (index, value) {
                    var _popover;
                    console.log(value.message);
                    _popover = $(value.element).popover({
                        trigger: "manual",
                        placement: "top",
                        content: value.message,
                        template: "<div class=\"popover\"><div class=\"arrow\"></div><div class=\"popover-inner\"><div class=\"popover-content\"><p></p></div></div></div>"
                    });
                    _popover.data("bs.popover").options.content = value.message;
                    return $(value.element).popover("show");
                });
            }
        });

        
    });
</script>

<div class="grid_3">
    <div class="container" id="main-container">
        <div id="regis-section">
        <ul class="nav nav-tabs">
            <li class="active"><a data-toggle="tab" href="#member">Member Signup</a></li>
            <li ><a data-toggle="tab" href="#hall">Hall Signup</a></li>
            <li ><a data-toggle="tab" href="#caterer">Caterer Signup</a></li>
        </ul>
        <div class="clearfix"></div>
        <br />
        <div class="tab-content">
            <div id="member" class="tab-pane fade in active">
                <div id="registration-area">
                    <div class="breadcrumb1">
                        <ul>
                            <a href="index.html"><i class="fa fa-home home_1"></i></a>
                            <span class="divider">&nbsp;|&nbsp;</span>
                            <li class="current-page">Member Registration</li>
                        </ul>
                    </div>
                        <small style="margin-left:17px;">All (*) Fields Are Required.</small>
                    <div class="clearfix"></div>
                        <div class="services" style="margin-top:20px;">
                            <div class="col-sm-6 login_left">
                                <form id="regis-form" action="\Account\Registration" method="post">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label for="edit-name">First name <span class="form-required" title="This field is required.">*</span></label>
                                            <input type="text" name="fname" value="" size="60" maxlength="60" class="form-control">
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label for="edit-name">Last name <span class="form-required" title="This field is required.">*</span></label>
                                            <input type="text" name="lname" value="" size="60" maxlength="60" class="form-text">
                                        </div>
                                    </div>
                                    <div class="clearfix"></div>
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label for="edit-name">Username <span class="form-required" title="This field is required.">*</span></label>
                                            <input type="text" name="username" value="" id="username" size="60" maxlength="60" class="form-text">
                                        </div>
                                    </div>
                                    <div class="clearfix"></div>
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label for="edit-pass">Password <span class="form-required" title="This field is required.">*</span></label>
                                            <input type="password" name="password" id="password" size="60" maxlength="128" class="form-text">
                                        </div>
                                    </div>
                                    <div class="clearfix"></div>
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label for="edit-pass">Re-enter Password <span class="form-required" title="This field is required.">*</span></label>
                                            <input type="password" name="repassword" size="60" maxlength="128" class="form-text">
                                        </div>
                                    </div>
                                    <div class="clearfix"></div>
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label for="edit-name">Email <span class="form-required" title="This field is required.">*</span></label>
                                            <input type="email" name="email" size="60" maxlength="60" class="form-text">
                                        </div>
                                    </div>
