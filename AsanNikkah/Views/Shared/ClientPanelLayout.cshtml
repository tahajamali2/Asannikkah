    <script>
        function ShowNotification(response,parentelement,second)
        {
            var bool = false;
            $("#alertbar_modal").remove();
            var exception = "<div class=\"alert alert-danger fade in alert-dismissable\" id=\"alertbar_modal\"><a href=\"#\" class=\"close\" data-dismiss=\"alert\" aria-label=\"close\" title=\"close\">×</a>";
            $.each($.parseJSON(response), function (index, element) {

                if (element.returntype == "success")
                {
                    exception = "<div class=\"alert alert-success fade in alert-dismissable\" id=\"alertbar_modal\"><a href=\"#\" class=\"close\" data-dismiss=\"alert\" aria-label=\"close\" title=\"close\">×</a><strong>Well done! </strong>" + element.message + "<br>";
                    bool = true;
                    return true;
                }

                else
                {
                    exception += "<strong>*</strong>" + element.message + "<br>";
                    bool = false;
                }

            });

            exception += "</div>";
            $(parentelement).prepend(exception);
            $('html, body').animate({
                scrollTop: $("html, body").offset().top
            }, 500);
            //setTimeout(function () { $("#alertbar_modal").fadeOut(300); $("#alertbar_modal").remove(); }, second*1000);
            return bool;
        }

        function isNumberKey(evt) {
            var charCode = (evt.which) ? evt.which : event.keyCode;
            if (charCode != 46 && charCode > 31 && (charCode < 48 || charCode > 57)) {
                return false;
            } else {
                return true;
            }
        }

        function Iserror(response)
        {
            var bool = false;
            var profileid = "";
            $.each($.parseJSON(response), function (index, element) {
                if (element.returntype == "success")
                {
                    bool = true;
                    profileid = element.profileid;
                    return true;
                }

                else
                {
                    bool = false;
                    profileid = "error";
                    return false;
                }
            });

            return profileid;
        }

        $(document).ready(function () {


        @RenderSection("Script", required: false)

           

            $.fn.serializeObject = function () {
                var o = {};
                var a = this.serializeArray();
                $.each(a, function () {
                    if (o[this.name] !== undefined) {
                        if (!o[this.name].push) {
                            o[this.name] = [o[this.name]];
                        }
                        o[this.name].push(this.value || '');
                    } else {
                        o[this.name] = this.value || '';
                    }
                });
                return o;
            };

            $(".rateYo").each(function (i, obj) {

                var val = $(obj).attr("data-rating");
                $(obj).rateYo({
                    rating: val,
                    readOnly: true
                });
            });


            $(".dropdown").hover(
                function () {
                    $('.dropdown-menu', this).stop(true, true).slideDown("fast");
                    $(this).toggleClass('open');
                },
                function () {
                    $('.dropdown-menu', this).stop(true, true).slideUp("fast");
                    $(this).toggleClass('open');
                }
            );
            var d = new Date();
            var d2 = new Date(70, 0, 1);


            $(".datepicker").datepicker({
                defaultDate:d2,
                changeMonth: true,
                changeYear: true,
                yearRange: '1940:' + (d.getFullYear() - 18).toString()
            });


            /*Update Modal Methods*/

            $("#caterer-basic-info-update-form").validate({
                rules: {
                    caterername: {
                        minlength: 3,
                        required: true
                    },
                    username: {
                        minlength: 5,
